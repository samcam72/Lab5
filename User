public class User {
    String name;
    int age;
    String email;
    String phone;

    public User(String name, int age, String email, String phone) {
        this.name = name;
        this.age = age;
        this.email = email;
        this.phone = phone;
    }

    public String toString() {
        return name + ", " + age + " years, " + email + ", " + phone;
    }
}
